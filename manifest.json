{
  "manifest_version": 2,
  "name": "페이지 내용을 읽고 수정할 수 있는가",
  "version": "0.1.0",

  "description": "페이지 내용을 읽을 수 있는지, 또, 수정할 수 있는 지 알아보기 위한 확장 프로그램.",
  "icons": {
    "32": "images/pagereadwrite32.png",
    "48": "images/pagereadwrite48.png"
  },
  "browser_action": {
    "default_icon": {
      "32": "images/pagereadwrite32.png",
      "48": "images/pagereadwrite48.png"
    },
    "default_title": "페이지 읽고 쓰자",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab"]
}
